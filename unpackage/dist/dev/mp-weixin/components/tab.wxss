
.tab{
	height: 50px;
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 99;
}
.tab-box{
	float: left;
	width: 20%;
	height: 50px;
	line-height: 50px;
	font-size: 16px;
	color: #fff;
	background-color: #000;
	text-align: center;
}
.icon-box{
	width: 60%;
	height: 30px;
	line-height: 30px;
	background-color: #fff;
	color: #000000;
	margin: 10px 20%;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
}

